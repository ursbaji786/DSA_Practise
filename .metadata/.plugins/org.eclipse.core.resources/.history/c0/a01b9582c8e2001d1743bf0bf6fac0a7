package week1;

public class ArrayContains {

	public static void main(String[] args) {
		
		int[] arr = {2,3,4,5};
		int num = 3;
		
		for(int i = 0 ; i < arr.length; i++) {
			if(arr[i]==num) {
				System.out.print("Given number exist");
			}
		}
		System.out.print("Given number NOT exist");
	}
}
