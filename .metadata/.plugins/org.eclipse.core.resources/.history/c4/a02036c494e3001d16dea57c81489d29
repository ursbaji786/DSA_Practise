package week1;

import java.util.Arrays;

public class RemoveValueAtGivenIndex {
	
	public static void main(String[] args) {
		
		int[] input = {1,2,3,4,5};
		int index = 2;
		int[] output = new int[input.length-1];
		
			
		
		for (int i = index; i < input.length-2; i++) {
			input[i] = input[i+1];			
		}
		System.out.print(Arrays.toString(input));
		
	}

}
